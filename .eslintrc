{
    "parser": "babel-eslint",
    "ecmaFeatures": {
        "modules": true,
        "jsx": true
    },
    "env": {
        "browser": true,
        "mocha": true,
        "es6": true
    },
    "globals": {
        "__filename": true,
        "process": true
    },
    "plugins": [
        "react"
    ],
    "extends": "eslint:recommended",
    "rules": {
        "consistent-return": 2,
        "curly": 2,
        "dot-location": [2, "property"],
        "dot-notation": 2,
        "eqeqeq": 2,
        "no-alert": 2,
        "no-caller": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-lone-blocks": 2,
        "no-multi-spaces": 2,
        "no-native-reassign": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-useless-call": 2,
        "no-void": 2,
        "no-with": 2,
        "radix": 2,

        "no-catch-shadow": 2,
        "no-shadow": 2,

        "array-bracket-spacing": 2,
        "brace-style": 2,
        "comma-spacing": 2,
        "comma-style": 2,
        "computed-property-spacing": 2,
        "indent": 2,
        "jsx-quotes": 2,
        "key-spacing": 2,
        "new-cap": 2,
        "new-parens": 2,
        "no-array-constructor": 2,
        "no-new-object": 2,
        "no-spaced-func": 2,
        "no-trailing-spaces": 2,
        "no-unneeded-ternary": 2,
        "object-curly-spacing": 2,
        "quotes": 2,
        "semi-spacing": 2,
        "semi": 2,
        "space-after-keywords": 2,
        "space-before-blocks": 2,
        "space-before-keywords": 2,
        "space-in-parens": 2,
        "space-infix-ops": 2,
        "space-return-throw-case": 2,
        "space-unary-ops": 2,

        "arrow-parens": [2, "as-needed"],
        "arrow-spacing": 2,
        "constructor-super": 2,
        "no-arrow-condition": 2,
        "no-class-assign": 2,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "no-this-before-super": 2,
        "object-shorthand": [2, "methods"],
        "prefer-arrow-callback": 2,
        "prefer-const": 1,
        "prefer-spread": 2,

        "react/jsx-curly-spacing": 2,
        "react/jsx-no-duplicate-props": 2,
        "react/jsx-no-undef": 2,
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/no-did-mount-set-state": 2,
        "react/no-did-update-set-state": 2,
        "react/no-direct-mutation-state": 2,
        "react/no-unknown-property": 2,
        "react/prefer-es6-class": 2,
        "react/react-in-jsx-scope": 2,
        "react/self-closing-comp": 2,
        "react/sort-comp": 2,
        "react/wrap-multilines": 2
    }
}