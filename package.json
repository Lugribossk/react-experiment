{
  "name": "react-experiment",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">= 10.4.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions"
  ],
  "scripts": {
    "start": "parcel src/index.html --out-dir target/serve --no-cache",

    "build": "yarn run build:ui && yarn run build:lambdas",
    "build:ui": "parcel build src/index.html --out-dir target/dist --public-url ./ --no-cache --no-source-maps",
    "build:lambdas": "parcel build src/lambdas/*.ts --out-dir target/lambdas --target node --no-cache --no-source-maps",
    "clean": "rimraf target/dist/*.* target/lambdas/*.*",

    "test": "yarn run lint && yarn run prettier-check",
    "lint": "tslint -p . \"{src,test}/**/*.{ts,tsx}\"",
    "prettier": "prettier --write \"{src,test}/**/*.{ts,tsx,json}\"",
    "prettier-check": "prettier --list-different \"{src,test}/**/*.{ts,tsx,json}\""
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-router-dom": "4.3.1",
    "semantic-ui-css": "2.3.2",
    "semantic-ui-react": "0.82.0",
    "tslib": "1.9.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.7",
    "@types/bluebird": "3.5.20",
    "@types/react": "16.3.16",
    "@types/react-dom": "16.0.6",
    "@types/react-router-dom": "4.2.7",
    "parcel-bundler": "1.9.6",
    "parcel-plugin-bundle-visualiser": "1.1.2",
    "parcel-plugin-typescript": "1.0.0",
    "prettier": "1.13.5",
    "react-hot-loader": "4.3.0",
    "rimraf": "2.6.2",
    "tslint": "5.10.0",
    "tslint-react": "3.6.0",
    "typescript": "2.9.1"
  }
}
