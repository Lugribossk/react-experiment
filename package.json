{
  "name": "react-experiment",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">= 10.4.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions"
  ],
  "scripts": {
    "build": "yarn run build:ui && yarn run build:lambda",
    "build:ui": "parcel build src/index.html --out-dir target/dist --public-url ./ --no-cache --no-source-maps",
    "build:lambda": "parcel build src/lambda/functions/*.ts --out-dir target/functions --target node --no-cache --no-source-maps",
    "clean": "rimraf target/dist/*.* target/functions/*.*",
    "lint": "tslint -p . \"{src,test}/**/*.{ts,tsx}\"",
    "prettier:fix": "prettier --write \"{src,test}/**/*.{ts,tsx,json}\"",
    "prettier:check": "prettier --list-different \"{src,test}/**/*.{ts,tsx,json}\"",
    "start:ui": "parcel serve src/index.html --out-dir target/serve --port 8080 --no-cache",
    "start:lambda": "cd src/lambda && node -r ts-node/register lambda-dev.ts",
    "test": "yarn run lint && yarn run prettier:check"
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-router-dom": "4.3.1",
    "semantic-ui-css": "2.3.2",
    "semantic-ui-react": "0.82.0",
    "tslib": "1.9.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.7",
    "@types/bluebird": "3.5.20",
    "@types/express": "4.16.0",
    "@types/react": "16.3.16",
    "@types/react-dom": "16.0.6",
    "@types/react-router-dom": "4.2.7",
    "body-parser": "1.18.3",
    "express": "4.16.3",
    "parcel-bundler": "1.9.6",
    "parcel-plugin-bundle-visualiser": "1.1.2",
    "parcel-plugin-typescript": "1.0.0",
    "prettier": "1.13.5",
    "react-hot-loader": "4.3.0",
    "rimraf": "2.6.2",
    "ts-node": "7.0.0",
    "tslint": "5.10.0",
    "tslint-react": "3.6.0",
    "typescript": "2.9.1"
  }
}
